####################

.PHONY: all opt byte archclean clean install depend html List N Tactic Z PrimalityTest elliptic num

byte:
	$(MAKE) all "OPT:=-byte"

opt:
	$(MAKE) all "OPT:=-opt"

install:
	mkdir -p $(COQLIB)/user-contrib
	(cd List; $(MAKE) INSTALLDEFAULTROOT=$(INSTALLDEFAULTROOT)/List install)
	(cd N; $(MAKE) INSTALLDEFAULTROOT=$(INSTALLDEFAULTROOT)/N install)
	(cd Tactic; $(MAKE) INSTALLDEFAULTROOT=$(INSTALLDEFAULTROOT)/Tactic install)
	(cd Z; $(MAKE) INSTALLDEFAULTROOT=$(INSTALLDEFAULTROOT)/Z install)
	(cd PrimalityTest; $(MAKE) INSTALLDEFAULTROOT=$(INSTALLDEFAULTROOT)/PrimalityTest install)
	(cd elliptic; $(MAKE) INSTALLDEFAULTROOT=$(INSTALLDEFAULTROOT)/elliptic install)
	(cd num; $(MAKE) INSTALLDEFAULTROOT=$(INSTALLDEFAULTROOT)/num install)

clean:
	rm -f $(CMOFILES) $(CMIFILES) $(CMXFILES) $(CMXSFILES) $(OFILES) $(VOFILES) $(VIFILES) $(GFILES) $(MLFILES:.ml=.cmo) $(MLFILES:.ml=.cmx) *~
	rm -f all.ps all-gal.ps all.pdf all-gal.pdf all.glob $(VFILES:.v=.glob) $(HTMLFILES) $(GHTMLFILES) $(VFILES:.v=.tex) $(VFILES:.v=.g.tex) $(VFILES:.v=.v.d)
	- rm -rf html
	(cd List ; $(MAKE) clean)
	(cd N ; $(MAKE) clean)
	(cd Tactic ; $(MAKE) clean)
	(cd Z ; $(MAKE) clean)
	(cd PrimalityTest ; $(MAKE) clean)
	(cd elliptic ; $(MAKE) clean)
	(cd num ; $(MAKE) clean)

archclean:
	rm -f *.cmx *.o
	(cd List ; $(MAKE) archclean)
	(cd N ; $(MAKE) archclean)
	(cd Tactic ; $(MAKE) archclean)
	(cd Z ; $(MAKE) archclean)
	(cd PrimalityTest ; $(MAKE) archclean)
	(cd elliptic ; $(MAKE) archclean)
	(cd num ; $(MAKE) archclean)


printenv: 
	@echo CAMLC =	$(CAMLC)
	@echo CAMLOPTC =	$(CAMLOPTC)
	@echo CAMLP4LIB =	$(CAMLP4LIB)

Makefile: Make
	mv -f Makefile Makefile.bak
	$(COQBIN)coq_makefile -f Make -o Makefile

	(cd List ; $(MAKE) Makefile)
	(cd N ; $(MAKE) Makefile)
	(cd Tactic ; $(MAKE) Makefile)
	(cd Z ; $(MAKE) Makefile)
	(cd PrimalityTest ; $(MAKE) Makefile)
	(cd elliptic ; $(MAKE) Makefile)
	(cd num ; $(MAKE) Makefile)

