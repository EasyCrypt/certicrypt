List:
	cd List ; $(MAKE) all

N:
	cd N ; $(MAKE) all

Tactic:
	cd Tactic ; $(MAKE) all

Z:
	cd Z ; $(MAKE) all

PrimalityTest:
	cd PrimalityTest ; $(MAKE) all

elliptic:
	cd elliptic ; $(MAKE) all

num:
	cd num ; $(MAKE) all

