.PHONY: all opt byte archclean clean install depend html ElGamal HElGamal HElGamal_ROM Switching FDH FDHcoron IBE OAEP OAEP-CCA ZK Indifferentiability

byte:
	$(MAKE) all "OPT:=-byte"

opt:
	$(MAKE) all "OPT:=-opt"

install:
	mkdir -p $(COQLIB)/user-contrib
	(cd ElGamal; $(MAKE) INSTALLDEFAULTROOT=$(INSTALLDEFAULTROOT)/ElGamal install)
	(cd HElGamal; $(MAKE) INSTALLDEFAULTROOT=$(INSTALLDEFAULTROOT)/HElGamal install)
	(cd HElGamal_ROM; $(MAKE) INSTALLDEFAULTROOT=$(INSTALLDEFAULTROOT)/HElGamal_ROM install)
	(cd Switching; $(MAKE) INSTALLDEFAULTROOT=$(INSTALLDEFAULTROOT)/Switching install)
	(cd FDH; $(MAKE) INSTALLDEFAULTROOT=$(INSTALLDEFAULTROOT)/FDH install)
	(cd FDHcoron; $(MAKE) INSTALLDEFAULTROOT=$(INSTALLDEFAULTROOT)/FDHcoron install)
	(cd IBE; $(MAKE) INSTALLDEFAULTROOT=$(INSTALLDEFAULTROOT)/IBE install)
	(cd OAEP; $(MAKE) INSTALLDEFAULTROOT=$(INSTALLDEFAULTROOT)/OAEP install)
	(cd OAEP-CCA; $(MAKE) INSTALLDEFAULTROOT=$(INSTALLDEFAULTROOT)/OAEP-CCA install)
	(cd ZK; $(MAKE) INSTALLDEFAULTROOT=$(INSTALLDEFAULTROOT)/ZK install)
	(cd Indifferentiability; $(MAKE) INSTALLDEFAULTROOT=$(INSTALLDEFAULTROOT)/Indifferentiability install)

clean:
	rm -f $(CMOFILES) $(CMIFILES) $(CMXFILES) $(CMXSFILES) $(OFILES) $(VOFILES) $(VIFILES) $(GFILES) $(MLFILES:.ml=.cmo) $(MLFILES:.ml=.cmx) *~
	rm -f all.ps all-gal.ps all.pdf all-gal.pdf all.glob $(VFILES:.v=.glob) $(HTMLFILES) $(GHTMLFILES) $(VFILES:.v=.tex) $(VFILES:.v=.g.tex) $(VFILES:.v=.v.d)
	- rm -rf html
	(cd ElGamal ; $(MAKE) clean)
	(cd HElGamal ; $(MAKE) clean)
	(cd HElGamal_ROM ; $(MAKE) clean)
	(cd Switching ; $(MAKE) clean)
	(cd FDH ; $(MAKE) clean)
	(cd FDHcoron ; $(MAKE) clean)
	(cd IBE ; $(MAKE) clean)
	(cd OAEP ; $(MAKE) clean)
	(cd OAEP-CCA ; $(MAKE) clean)
	(cd ZK ; $(MAKE) clean)
	(cd Indifferentiability ; $(MAKE) clean)

archclean:
	rm -f *.cmx *.o
	(cd ElGamal ; $(MAKE) archclean)
	(cd HElGamal ; $(MAKE) archclean)
	(cd HElGamal_ROM ; $(MAKE) archclean)
	(cd Switching ; $(MAKE) archclean)
	(cd FDH ; $(MAKE) archclean)
	(cd FDHcoron ; $(MAKE) archclean)
	(cd IBE ; $(MAKE) archclean)
	(cd OAEP ; $(MAKE) archclean)
	(cd OAEP-CCA ; $(MAKE) archclean)
	(cd ZK ; $(MAKE) archclean)
	(cd Indifferentiability ; $(MAKE) archclean)


printenv: 
	@echo CAMLC =	$(CAMLC)
	@echo CAMLOPTC =	$(CAMLOPTC)
	@echo CAMLP4LIB =	$(CAMLP4LIB)

Makefile: Make
	mv -f Makefile Makefile.bak
	$(COQBIN)coq_makefile -f Make -o Makefile

	(cd ElGamal ; $(MAKE) Makefile)
	(cd HElGamal ; $(MAKE) Makefile)
	(cd HElGamal_ROM ; $(MAKE) Makefile)
